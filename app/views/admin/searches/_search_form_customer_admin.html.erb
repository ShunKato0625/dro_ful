<div class="col-sm-12 col-12 form-inline d-block text-right">
  <%= form_with url: admin_search_customer_path, method: :get, local: true do |f| %>
    <%= f.text_field :keyword , placeholder:"ユーザー名（ニックネーム)で検索", class:"col-sm-8 col-10" %>
    <%= f.button :type => "submit", class: "btn btn-outline-success" do %>
      <i class="fas fa-search"></i>
    <% end %>
  <% end %>
</div>